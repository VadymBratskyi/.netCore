@model IEnumerable<WebIdentity.Models.User>
 
<h2>Список пользователей</h2>
<table class="table">
  @foreach (var user in Model)
  {
    <tr>
      <td>@user.Email</td>
      <td>
        <a class="btn btn-sm btn-primary" href="@Url.Action("Edit","Roles", new {userid = user.Id})">Права доступа</a>
      </td>
    </tr>
  }
</table>