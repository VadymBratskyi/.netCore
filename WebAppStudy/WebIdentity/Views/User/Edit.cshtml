@model WebIdentity.ViewModels.EditUserViewModel
@{
  ViewBag.Title = "Редактирование пользователя";
}
<form action="Edit" method="post">
  <div class="form-group">
    <input type="hidden" id="Id" name="Id" value="@Model.Id"/>
  </div>
  <div class="form-group">
    <label for="Email" class="control-label">Email</label>
    <input type="text" id="Email" name="Email" class="form-control" value="@Model.Email" />
  </div>
  <div class="form-group">
    <label for="Year" class="control-label">Год рождения</label>
    <input type="number" id="Year" name="Year" class="form-control" value="@Model.Year"/>
  </div>
  <div class="form-group">
    <input type="submit" value="Сохранить" class="btn btn-default" />
  </div>
</form>